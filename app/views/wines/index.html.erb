<!doctype html>

<html>
<head>

	<title> Engineering Wine Society</title>
	<meta charset="utf-8">

</head>
<!-- DataTables, fattable, infinity.js -->
<body>

  <%= render 'layouts/header' %>


  <br>
  <br>
  <br>
  <br>
  <br>
  <br>
  <br>
  <br>
  <br>
  <br>



	 <div id="tbl">

	<table class= "table table-striped table-hover" id="example">
	<tr>
     	<th><center>Label</center></th>
      <th><center>Name</center></th>
      <th><center>Varietal</center></th>
      <th><center>Winery</center></th>
	</tr>

	<% @wines.each do |wine| %>

	<%if wine.user_id == current_user.id %>

	<tr>
			<th><center><%= image_tag("#{wine.url}", :alt => "Holder") %></center></th>
			<th><center><%=link_to "#{wine.name}", wine%></center></th>
			<th><center><%=wine.varietal%></center></th>
			<th><center><%=wine.winery%></center></th>
	</tr>
	<%end%>
	<% end %>




	</table>




</body>


</html>
